
<div id="map"></div>

<script type="text/javascript">
  var data = <%= @venues.to_json %>
  console.log(data,"from venues")
  initMap(data)
</script>
<hr>
<div id="venueLinks">
<ul>
<% @venues.each do | venue | %>
  <a href="/venues/<%= venue.id %>">
  <li class="limeLinks">
    Venue name: <strong><%= venue.name %>,</strong>
    Name of event: <strong><%= venue.venue_name %>,</strong>
    What genre: <strong><%= venue.genre %>,</strong>
    Location: <strong><%= venue.location %></strong>
  </li></a>
<% end %>
</ul>
