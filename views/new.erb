        <form class="form-horizontal" action="/venues" method="POST">
          <fieldset>

            <!-- Form Name -->
            <legend>Add New Venues</legend>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="name">Name</label>
              <div class="col-md-4">
              <input id="name" name="name" type="text" placeholder="" class="form-control input-md" required="">

              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="artistName">Venue Name</label>
              <div class="col-md-4">
              <input id="textinput" name="venue_name" type="text" placeholder="" class="form-control input-md">

              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="Location">Location</label>
              <div class="col-md-4">
              <input id="Location" name="location" type="text" placeholder="City, State, Country " class="form-control input-md">

              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="days_available">Days Available</label>
              <div class="col-md-4">
              <input id="Location" name="days_available " type="text" placeholder="Every Wednesday, Friday etc." class="form-control input-md">

              </div>
            </div>

            <!-- Textarea -->
            <div class="form-group">
              <label class="col-md-4 control-label" for="age_restriction">Age Restriction(check for yes)</label>
              <div class="col-md-4">
                <input id="age_restriction" name="age_restriction" placeholder="enter true or false" class="form-control input-md">
              </div>
            </div>

            <!-- Textarea -->
            <div class="form-group">
              <label class="col-md-4 control-label" for="genre">Genres (separated by commas)</label>
              <div class="col-md-4">
                <textarea class="form-control" id="genre" name="venue[genre]" placeholder="rock, pop, All"></textarea>
              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="registration_required">Registration Required(check for yes)</label>
              <div class="col-md-4">
              <input id="registration_required" name="registration_required" type="text" placeholder="enter true or false" class="form-control input-md">

              </div>
            </div>

            <!-- Text input-->
            <div class="form-group">
              <label class="col-md-4 control-label" for="website">Website</label>
              <div class="col-md-4">
              <input id="website" name="website" type="text" placeholder="" class="form-control input-md">

              </div>
            </div>

            <!-- Textarea -->
            <div class="form-group">
              <label class="col-md-4 control-label" for="special_instructions">Special Instructions</label>
              <div class="col-md-4">
                <textarea class="form-control" id="special_instructions" name="special_instructions">How do artists get set up?(what is the age restriction/contact person?)</textarea>
              </div>
            </div>

            <!-- Button -->
            <div class="form-group">
              <label class="col-md-4 control-label" for="singlebutton">Save Venue</label>
              <div class="col-md-4">
                <button id="singlebutton" class="btn btn-primary">Submit</button>

              </div>
            </div>



          </fieldset>
        </form>
      </div>
      </div>
    </section>








